{
  "name": "fastify-product-service",
  "version": "1.0.0",
  "description": "Product service with Fastify, TypeScript, and OpenTelemetry",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node --require ./dist/utils/JsonDiagConsoleLogger.js --require ./dist/otel.js dist/server.js",
    "dev": "tsx watch src/server.ts",
    "dev:no-otel": "tsx watch src/server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/core": "^2.1.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.205.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.205.0",
    "@opentelemetry/instrumentation": "^0.205.0",
    "@opentelemetry/instrumentation-fastify": "^0.50.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/instrumentation-pg": "^0.58.0",
    "@opentelemetry/instrumentation-pino": "^0.52.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "dotenv": "^16.3.1",
    "fastify": "^5.6.0",
    "fastify-plugin": "^5.0.1",
    "pg": "^8.11.3",
    "pino": "^9.9.5",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^30.1.3",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.2",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ]
  }
}
